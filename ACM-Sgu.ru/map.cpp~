#include <iostream>
#include <map>

using namespace std;

map<int,int> k;

int main()
{
	pair<int,int> f;
	f.first=1;
	f.second=2;
	pair<int,int> v;
	v.first=3;
	v.second=4;
	k.insert(f);
	k.insert(v);
	if(k.find(1)!=k.end())
	{
		cout << k.size() << " :O " << endl;
		cout << " :) " << endl;
	}
	else
		cout << " :| " << endl;
}
